class Solution {
    public String reverseWords(String s) {
        if (s.isEmpty()) {
            return "";
        }

        String[] words = s.split(" +"); // for more then 1 spaces
        StringBuilder res = new StringBuilder();
        
        for(int i =words.length-1; i> 0 ;i--){
            res.append(words[i]);
            res.append(" ");

        }
          res.append(words[0]);
      return res.toString().trim();  
    }
}
