class Solution {
    public boolean isValid(String s) {
        Stack<Character> st = new Stack<>();
        for( Character c :s.toCharArray()){

            if(c == '(' || c == '[' || c == '{'){
                st.push(c);
            }else{
                 if(st.isEmpty())
                     return false;
                 else if ( c==')' && st.peek()!='(' || c== ']' && st.peek()!='['
                 || c== '}' && st.peek()!= '{' )
                   return false;
                else{
                    st.pop();
                    }

            }
        }
             if(st.isEmpty())
                     return true;
             return false;
        }
        
    }
    // o(n )

